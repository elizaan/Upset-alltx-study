{
    "$schema": "https://raw.githubusercontent.com/reVISit-studies/study/main/src/parser/StudyConfigSchema.json",
    "studyMetadata": {
        "title": "Upset Alttext User Survey",
        "version": "pilot",
        "authors": [
            "The reVISit Team"
        ],
        "date": "2024-03-19",
        "description": "A sample questionnaire study",
        "organizations": [
            "University of Utah"
        ]
    },
    "uiConfig": {
        "contactEmail": "test@test.com",
        "helpTextPath": "Upset-Alttext-User-Survey/assets/help.md",
        "logoPath": "revisitAssets/revisitLogoSquare.svg",
        "withProgressBar": true,
        "autoDownloadStudy": false,
        "sidebar": true
    },
    "components": {
        "Introduction": {
            "type": "markdown",
            "path": "Upset-Alttext-User-Survey/assets/introduction.md",
            "response": []
        },
        "Visual-Only-Questionnaire 1": {
            "description": "Content: Visual Only",
            "instruction": "Please answer the following question about the chart:",
            "type": "image",
            "path": "Upset-Alttext-User-Survey/assets/VO.png",
            "style": {
                "width": "450px",
                "height": "600px"
            },
            "nextButtonLocation": "sidebar",
            "response": [
                {
                    "id": "voq1",
                    "prompt": "1. What are your insights and take-aways from this chart for this data?",
                    "required": true,
                    "location": "sidebar",
                    "type": "longText",
                    "placeholder": "Please enter your answer here"
                }
            ]
        },
        "Visual-Only-Questionnaire 2": {
            "description": "Content: Visual Only",
            "instruction": "Please answer the following questions about the chart:",
            "type": "image",
            "path": "Upset-Alttext-User-Survey/assets/VO.png",
            "style": {
                "width": "450px",
                "height": "600px"
            },
            "nextButtonLocation": "sidebar",
            "response": [
                {
                    "id": "voq1",
                    "prompt": "2. How many sets are shown in the upset plot?",
                    "required": true,
                    "location": "sidebar",
                    "type": "numerical",
                    "placeholder": "Please enter your answer here",
                    "min": 0,
                    "max": 100

                },
                {
                    "id": "voq2",
                    "prompt": "3. How many intersections are seen in the UpSet plot?",
                    "required": true,
                    "location": "sidebar",
                    "type": "numerical",
                    "placeholder": "Please enter your answer here",
                    "min": 0,
                    "max": 100

                },
                {
                    "id": "voq3",
                    "prompt": "4. What is the largest set?",
                    "required": true,
                    "location": "sidebar",
                    "type": "shortText",
                    "placeholder": "Please enter your answer here"
                },
                {
                    "id": "voq4",
                    "prompt": "5. What is the largest intersection? (e.g., set_A and set_B)",
                    "required": true,
                    "location": "sidebar",
                    "type": "shortText",
                    "placeholder": "Please enter your answer here"
                },
                {
                    "id": "voq5",
                    "prompt": "6. How large is the largest intersection?",
                    "required": true,
                    "location": "sidebar",
                    "type": "numerical",
                    "placeholder": "Please enter your answer here",
                    "min": 0,
                    "max": 1000000000
                },
                {
                    "id": "voq6",
                    "prompt": "7. How many sets make up the largest intersection?",
                    "required": true,
                    "location": "sidebar",
                    "type": "dropdown",
                    "placeholder": "Please choose your answer",
                    "options": [
                        {
                            "label": "It is only of a single set.",
                            "value": "Independent set"
                        },
                        {
                            "label": "It is the intersection of 2-3 sets.",
                            "value": "Low set"
                        },
                        {
                            "label": "It is the intersection of many sets.",
                            "value": "Medium / High order set"
                        }
                        
                    ]
                },
                {
                    "id": "voq7",
                    "prompt": "8. How similar are the set sizes?",
                    "required": true,
                    "location": "sidebar",
                    "type": "dropdown",
                    "placeholder": "Please choose your answer",
                    "options": [
                        {
                            "label": "Roughly equal (all the set sizes look similar).",
                            "value": "Identical"
                        },
                        {
                            "label": "Diverging a bit (the sizes of the largest set and the smallest set don't differ largely).",
                            "value": "Diverging a bit"
                        },
                        {
                            "label": "Diverging a lot (the sizes of the largest set and the smallest set differ largely).",
                            "value": "Diverging a lot"
                        }
                    ]
                },
                {
                    "id": "voq8",
                    "prompt": "9. Is the largest set present in the largest intersection?",
                    "required": true,
                    "location": "sidebar",
                    "type": "radio",
                    "options": [
                        {
                            "label": "Yes",
                            "value": "1"
                        },
                        {
                            "label": "No",
                            "value": "2"
                        },
                        {
                            "label": "Not able to extract",
                            "value": "3"
                        }
                    ]
                },
                {
                    "id": "voq9",
                    "prompt": "10. Is the all-set intersection (intersection having all the sets) present? ",
                    "required": true,
                    "location": "sidebar",
                    "type": "radio",
                    "options": [
                        {
                            "label": "Yes",
                            "value": "1"
                        },
                        {
                            "label": "No",
                            "value": "2"
                        },
                        {
                            "label": "Not able to extract",
                            "value": "3"
                        }
                    ]
                }
            ]  
                
        },
        "Visual-Only-Questionnaire 3": {
            "description": "Content: Visual Only",
            "instruction": "Rate the content:",
            "type": "image",
            "path": "Upset-Alttext-User-Survey/assets/VO.png",
            "style": {
                "width": "450px",
                "height": "600px"
            },
            "nextButtonLocation": "sidebar",
            "response": [
                {
                    "id": "voq1",
                    "prompt": "11. On a scale of 1 to 5, how confident are you in your answers (1 = Not at all confident, 5 = Extremely confident)",
                    "required": true,
                    "location": "sidebar",
                    "type": "likert",
                    "preset": 5,
                    "leftLabel": "Not at all confident",
                    "rightLabel": "Extremely confident"

                },
                {
                    "id": "voq2",
                    "prompt": "12. On a scale of 1 to 5, how well did you understand the information presented? (1 = Not at all understood, 5 = Completely understood)",
                    "required": true,
                    "location": "sidebar",
                    "type": "likert",
                    "preset": 5,
                    "leftLabel": "Not at all understood",
                    "rightLabel": "Completely understood"
                },
                {
                    "id": "voq3",
                    "prompt": "13. On a scale of 1 to 5, how effective was the chart at conveying information? (1 = Not at all effective, 5 = Extremely effective)",
                    "required": true,
                    "location": "sidebar",
                    "type": "likert",
                    "preset": 5,
                    "leftLabel": "Not at all effective",
                    "rightLabel": "Extremely effective"
                }
            ]
        }
       
    },
    "sequence": {
        "order": "fixed",
        "components": [
            "Introduction",
            "Visual-Only-Questionnaire 1",
            "Visual-Only-Questionnaire 2",
            "Visual-Only-Questionnaire 3"
        ]
    }
}