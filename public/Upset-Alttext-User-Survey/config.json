                {
                    "id": "voq1",
                    "prompt": "How many sets are shown in the upset plot?",
                    "required": true,
                    "location": "sidebar",
                    "type": "numerical",
                    "placeholder": "Please enter your answer here",
                    "min": 0,
                    "max": 100

                },
                {
                    "id": "voq2",
                    "prompt": "How many intersections are seen in the UpSet plot?",
                    "required": true,
                    "location": "sidebar",
                    "type": "numerical",
                    "placeholder": "Please enter your answer here",
                    "min": 0,
                    "max": 100

                },
                {
                    "id": "voq3",
                    "prompt": "What is the largest set?",
                    "required": true,
                    "location": "sidebar",
                    "type": "shortText",
                    "placeholder": "Please enter your answer here"
                },
                {
                    "id": "voq4",
                    "prompt": "What is the largest intersection?",
                    "secondaryText": "e.g., set_A and set_B",
                    "required": true,
                    "location": "sidebar",
                    "type": "shortText",
                    "placeholder": "Please enter your answer here"
                },
                {
                    "id": "voq5",
                    "prompt": "How large is the largest intersection?",
                    "required": true,
                    "location": "sidebar",
                    "type": "numerical",
                    "placeholder": "Please enter your answer here",
                    "min": 0,
                    "max": 1000000000
                },
                {
                    "id": "voq6",
                    "prompt": "How many sets make up the largest intersection?",
                    "required": true,
                    "location": "sidebar",
                    "type": "dropdown",
                    "placeholder": "Please choose your answer",
                    "options": [
                        {
                            "label": "It is only of a single set.",
                            "value": "Independent set"
                        },
                        {
                            "label": "It is the intersection of 2-3 sets.",
                            "value": "Low set"
                        },
                        {
                            "label": "It is the intersection of many sets.",
                            "value": "Medium / High order set"
                        }
                        
                    ]
                },
                {
                    "id": "voq7",
                    "prompt": "How similar are the set sizes?",
                    "required": true,
                    "location": "sidebar",
                    "type": "dropdown",
                    "placeholder": "Please choose your answer",
                    "options": [
                        {
                            "label": "Roughly equal (all the set sizes look similar).",
                            "value": "Identical"
                        },
                        {
                            "label": "Diverging a bit (the sizes of the largest set and the smallest set don't differ largely).",
                            "value": "Diverging a bit"
                        },
                        {
                            "label": "Diverging a lot (the sizes of the largest set and the smallest set differ largely).",
                            "value": "Diverging a lot"
                        }
                    ]
                },
                {
                    "id": "voq8",
                    "prompt": "Is the largest set present in the largest intersection?",
                    "required": true,
                    "location": "sidebar",
                    "type": "radio",
                    "options": [
                        {
                            "label": "Yes",
                            "value": "1"
                        },
                        {
                            "label": "No",
                            "value": "2"
                        }
                    ]
                },
                {
                    "id": "voq9",
                    "prompt": "Is the all-set intersection (intersection having all the sets) present? ",
                    "required": true,
                    "location": "sidebar",
                    "type": "radio",
                    "options": [
                        {
